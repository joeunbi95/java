package day16;

import java.util.Arrays;
import java.util.stream.Stream;

public class StreamEx02 {

	public static void main(String[] args) {
		/* 배열에서 짝수만 출력(정렬)
		 * 중복되는 값을 삭제
		 * */
		int[] arr = {1,2,3,4,5,6,7,8,9};
		
		Arrays.stream(arr).sorted().forEach(n->System.out.println(n+" "));
	}

}
