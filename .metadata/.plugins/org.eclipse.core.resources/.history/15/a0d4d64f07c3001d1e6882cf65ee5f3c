package product;

public class Product {
	
	private String menu;
	private int price;
	
	public Product(){}
	
	public Product(String menu, int price) {
		this.menu=menu;
		this.price=price;		
	}
	
	public void foodPrint() {
		System.out.println(menu+":"+"가격:"+price);
	}

	public String getMenu() {
		return menu;
	}

	public void setMenu(String menu) {
		this.menu = menu;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "product [" + menu + ":" + price + "]";
	}
	
	
}
